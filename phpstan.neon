includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app/

    # Level 9 is the highest level
    level: 6

#    ignoreErrors:
#        - '#PHPDoc tag @var#'
#
    excludePaths:
        - ./app/Nova/Resource.php
#
#    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    automaticallyRemoveInferableTypes: false
services:
    factory: PhpStan\Parser\ParserFactory::create(['skipPhpDoc' => true])
